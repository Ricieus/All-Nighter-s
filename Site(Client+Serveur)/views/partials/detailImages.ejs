<div class="container mb-5">
    <div style="width: 40rem; float: left;">
        <% itemsMongo.forEach(car=> { %>
            <img src="/images/<%= car.images[0] %>" class="card-img-top">
            <% }); %>
    </div>
    <div style="width: 40rem; float: right; justify-content: center;">
        <div>
            <div class="row">
                <% itemsMongo.forEach(car=> { %>
                    <div class="col-md-5 m-3">
                        <img src="/images/<%= car.images[0] %>" class="card-img-top">
                    </div>
                    <div class="col-md-5 m-3">
                        <img src="/images/<%= car.images[1] %>" class="card-img-top">
                    </div>
                    <% }); %>
            </div>
        </div>
        <div>
            <div class="row">
                <% itemsMongo.forEach(car=> { %>
                    <div class="col-md-5 m-3">
                        <img src="/images/<%= car.images[2] %>" class="card-img-top">
                    </div>
                    <div class="col-md-5 m-3">
                        <img src="/images/<%= car.images[3] %>" class="card-img-top">
                    </div>
                    <% }); %>
            </div>
        </div>
    </div>
</div>
<div class="container mt-5">
    <% if (itemsMySQL && itemsMongo && itemsMySQL.length> 0) { %>
        <% const car1=itemsMySQL[0]; %>
            <h1>
                <%= car1.prix %>$
            </h1>

            <% }; %>
</div>