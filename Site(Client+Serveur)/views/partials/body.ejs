<body>
    <style>
        .sectionHaut {
            height: 100vh;
            background-size: cover;
            background-image: url("https://collectingcars.imgix.net/022036/DSC-76322.jpg?fit=clip&w=2000&auto=format,compress&cs=srgb&q=85");
            vertical-align: middle;
            align-items: center;
            align-content: center;
        }

        .boiteMilieu {
            height: 10%;
            background-color: #fff;
            width: 80%;
            margin-top: 20%;
            border-radius: 25px;
        }

        .testDiv {
            height: 100vh;
        }

        .container .carousel {
            margin-top: 5%;
        }

        .carousel-item img {
            aspect-ratio: 150/100;

        }


    </style>

    


    <div class="sectionHaut">
        <div class="h-100 d-flex align-items-center justify-content-center">
            <div class="boiteMilieu">
                <div class="container">
                    <div class="row" style="margin-top: 1%;">
                        <div class="col-md-3">
                            ONE
                        </div>
                        <div class="col-md-3">
                            TWO
                        </div>
                        <div class="col-md-3">
                            THREE
                        </div>
                        <div class="col-md-3">
                            FOUR
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="testDiv">
        <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
        <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <!-- Bootstrap JS -->
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
        <div class="container mt-5">
            <h1>Voitures</h1>
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100 h-100"
                            src="https://imagenes.km77.com/fotos/bbtcontent/clippingnew/KM7KPH20210624_0005/full-original.jpg">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>
                                Honda Civic 2023
                            </h5>
                            <p>Prix de base: 25 729$ + taxes.</p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 h-100"
                            src="https://www.forbes.com/wheels/wp-content/uploads/2022/07/2023_Honda_CRV_1.jpg">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>Honda CR-V 2023</h5>
                            <p>Prix de base: 33 021$ + taxes.</p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 h-100"
                            src="https://mundomotorescr.com/wp-content/uploads/2022/11/Honda-Pilot-2023.jpg">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>Honda Pilot 2023</h5>
                            <p>Prix de base: 38 729$ + taxes.</p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 h-100"
                            src="https://quatrorodas.abril.com.br/wp-content/uploads/2021/02/unnamed-7.jpg?quality=70&strip=info">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>Toyota Corrolla 2023</h5>
                            <p>Prix de base: 24 729$ + taxes.</p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 h-100"
                            src="https://th.bing.com/th/id/R.d9402954163883726b5612dc01d85ef1?rik=%2f7vDKVyROguj3w&riu=http%3a%2f%2fcdn.carbuzz.com%2fgallery-images%2f1600%2f570000%2f0%2f570040.jpg&ehk=RY5eenSJj3w6pGu2TvYQEBjYJrZATH6UiSFqx%2btiWqM%3d&risl=&pid=ImgRaw&r=0">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>Toyota Rav4 2023</h5>
                            <p>Prix de base: 34 729$ + taxes.</p>
                        </div>
                    </div>
                </div>
                <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>

    <div class="container mt-5">
        <br>
        <h2>Nous contacter</h2>
        <form class="form-vertical">
            <div class="d-flex justify-content-center">
                <div class="m-2">
                    <label>Courriel:</label>
                    <input type="email" class="form-control">
                </div>

                <div class="m-2">
                    <label>Raison de contact:</label>
                    <select class="form-select">
                        <option value="Select">-----Sélectionnez-----</option>
                        <option value="question">Je veux demander une question</option>
                        <option value="postuler">Je veux postuler pour un emploi</option>
                        <option value="plainte">Je veux me plaindre</option>
                        <option value="commentaire">Je veux laisser un commentaire</option>
                    </select>
                </div>
                <div class="m-2">
                    <label>Choisir un rendez-vous:</label>
                    <input type="date" class="form-control">
                </div>
                <div class="m-2">
                    <label>Message supplémentaire:</label>
                    <textarea class="form-control"></textarea>
                </div>
                <div class="m-5">
                    <button class="btn btn-primary">
                        Envoyer
                    </button>
                </div>
            </div>
        </form>
    </div>
</body>